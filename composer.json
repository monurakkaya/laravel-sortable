{
    "name": "monurakkaya/laravel-sortable",
    "description": "Enables simple sort for your Eloquent models",
    "type": "library",
    "keywords": [
        "sort",
        "sortable",
        "model",
        "eloquent",
        "laravel"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Monurakkaya\\Sortable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monurakkaya\\Sortable\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mehmet Onur Akkaya",
            "email": "monurakkaya@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Monurakkaya\\Sortable\\Providers\\SortableServiceProvider"
            ]
        }
    }
}
